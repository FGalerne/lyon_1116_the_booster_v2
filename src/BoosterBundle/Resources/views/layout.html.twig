{% extends('::base.html.twig') %}

{% block stylesheets %}
    <link rel="stylesheet" href="{{ asset('/css/register.css')}}">
{% endblock %}

{% block content%}
    <div class="ml_title">
        {% block h1 %} {% endblock %}
        <hr>
    </div>

    <div class="container">
        <div class="col-xs-offset-1 col-xs-10 col-md-offset-4 col-md-4">
            <div class="row">
                    {% block fos_user_content %}
                    {% endblock fos_user_content %}
            </div>
        </div>
    </div>
    <div class="container">
        <div class="col-xs-offset-1 col-xs-10">
            <div class="row">
                <div class="validation-height">
                    {% block validation_content %}
                    {% endblock validation_content %}
                </div>
            </div>
        </div>
    </div>



{% endblock %}
{% block javascripts %}
    <script type="text/javascript" src="{{ asset('js/form.js')}}"></script>
{% endblock %}